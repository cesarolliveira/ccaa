<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="icon" href="{{ asset('build/default/ccaa-icon.png') }}" type="image/gif" sizes="16x16"> 

        {% block stylesheets %}
            {{ encore_entry_link_tags('base') }}
            {{ encore_entry_link_tags('styles/system/app') }}
        {% endblock %}
    </head>

    {% block body %}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('base') }}
    {% endblock %}
</html>
